pattern 
1 2 3 4 5
10 9 8 7 6
11 12 13 14 15
20 19 18 17 16
21 22 23 24 25

k=1
l=[]
for i in range(1,6):
    if i%2!=0:
        for j in range(1,6):
            if k<=25:
                l.append(k)
            k+=1
        print(*l)
    l=[]
            
    if i%2==0:
        for j in range(5,0,-1):
            if k<=25:
                l.append(k)
            k+=1
        print(*l[::-1])
        l=[]
